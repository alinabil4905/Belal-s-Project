<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.TodlProjectV1" parent="Theme.MaterialComponents.Light">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#E6F8FF</item>
        <item name="android:textColorPrimary">#7B7B7B</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">#0049FF</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#E6F8FF</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Splash" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="android:statusBarColor" tools:targetApi="l">#E6F8FF</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
    </style>
</resources>